var from{{.fromFieldName}} *{{.toFullFieldType}}
if from.{{.fromFieldName}} != nil {
    {{- if .isError -}}
    res, err := {{.conversionFunction}}
    if err != nil {
        return {{.toModelName}}{}, err
    }
    {{else}}
    res := {{.conversionFunction}}
    {{- end}}
    from{{.fromFieldName}} = &res
}
